    <connection>dbrgca_connection</connection>
    <sql>-- Cantidad de Profesores por colectivo

select nombre_carrera, curso, anno_academico, modalidad, pk_id_persona, nombres, apellidos, nombre_publicacion as nomb_revista, indexada,
	CAST(COUNT(pk_fk_publicacion) AS FLOAT) as cant_publicaciones
from
	(
	select nombre_carrera, curso, anno_academico, modalidad, pk_id_persona, nombres, apellidos
		from tb_claustro_colectivo
			inner join tb_colectivo_anno on pk_fk_colectivo_anno = pk_id_colectivo_anno
				inner join tb_carrera on fk_id_carrera = pk_id_carrera
					inner join tb_persona on pk_fk_profesor = pk_id_persona
	) as colectivos_personas
left outer join
	(select pk_fk_autor, pk_fk_publicacion, nombre_publicacion, indexada
	from tb_autores_publicacion
		inner join tb_publicacion	
		on pk_fk_publicacion = pk_id_publicacion and indexada is not null
	) as publicaciones
on colectivos_personas.pk_id_persona = publicaciones.pk_fk_autor
group by nombre_carrera, curso, anno_academico, modalidad, pk_id_persona, nombres, apellidos, nombre_publicacion, indexada</sql>
